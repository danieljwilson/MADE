        <Param name="name" updates="None" val="code_2" valType="code"/>
        <Param name="Begin Routine" updates="constant" val="theseKeys=&quot;&quot;&amp;#10;shift_flag = False&amp;#10;input_text_faceVal.alignHoriz = 'center'&amp;#10;&amp;#10;# Reset index if counter = 9 (9 house exemplars)&amp;#10;if counter == 9:&amp;#10;    counter = 0&amp;#10;    roundNum = roundNum + 1&amp;#10;    correct_num = 0&amp;#10;    earnings = 0&amp;#10;&amp;#10;# For new set of learning trials, create random array of faces&amp;#10;if counter == 0:&amp;#10;    selector = np.arange(9)&amp;#10;    np.random.shuffle(selector)&amp;#10;    correct_num = 0&amp;#10;&amp;#10;learn_image = house_df.exemplar[selector[counter]]&amp;#10;actualValue = round(house_df.value[selector[counter]], 2)&amp;#10;&amp;#10;# If 5 rounds, then STOP the experiment&amp;#10;if roundNum == 5:&amp;#10;    stopText = &quot;STOP \nGET RA&quot;&amp;#10;    learn_image = inputText = &quot;images/exemplars/end_trial.jpg&quot;&amp;#10;" valType="extendedCode"/>
