    <Routine name="LearnFaces_02">
      <CodeComponent name="code">
        <Param name="Begin Experiment" updates="constant" val="counter = 0&amp;#10;roundNum = 1&amp;#10;stopText = &quot;&quot;&amp;#10;learn_image = &quot;images/exemplars/faces/CFD-AF-200-228-N.jpg&quot;&amp;#10;" valType="extendedCode"/>
        <Param name="name" updates="None" val="code" valType="code"/>
        <Param name="Begin Routine" updates="constant" val="theseKeys=&quot;&quot;&amp;#10;shift_flag = False&amp;#10;input_text_faceVal.alignHoriz = 'center'&amp;#10;&amp;#10;# Skip segment&amp;#10;#learn_faces.finished = 1&amp;#10;&amp;#10;# Reset index if counter = 9 (9 face exemplars)&amp;#10;if counter == 9:&amp;#10;    counter = 0&amp;#10;    roundNum = roundNum + 1&amp;#10;    correct_num = 0&amp;#10;    earnings = 0&amp;#10;&amp;#10;# For new set of learning trials, create random array of faces&amp;#10;if counter == 0:&amp;#10;    selector = np.arange(9)&amp;#10;    np.random.shuffle(selector)&amp;#10;    correct_num = 0&amp;#10;&amp;#10;learn_image = face_df.exemplar[selector[counter]]&amp;#10;actualValue = round(face_df.value[selector[counter]], 2)&amp;#10;&amp;#10;# If 5 rounds, then STOP the experiment&amp;#10;if roundNum == 5:&amp;#10;    stopText = &quot;STOP \nGET RA&quot;&amp;#10;    learn_image = inputText = &quot;images/exemplars/end_trial.jpg&quot;&amp;#10;" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="guessValue = round(float(inputText),2)&amp;#10;&amp;#10;# Check if Correct&amp;#10;if guessValue == actualValue:&amp;#10;    correct = 1&amp;#10;    correct_num = correct_num + 1&amp;#10;    earnings = earnings + 0.25&amp;#10;else:&amp;#10;    correct = 0&amp;#10;    earnings = earnings - 0.25&amp;#10;# Store Results&amp;#10;thisExp.addData('guessFaceVal', inputText)&amp;#10;thisExp.addData('actFaceVal', actualValue)&amp;#10;thisExp.addData('image', learn_image)&amp;#10;thisExp.addData('correct', correct)&amp;#10;thisExp.addData('total_correct', correct_num)&amp;#10;thisExp.addData('trial', counter)&amp;#10;thisExp.addData('round', roundNum)&amp;#10;thisExp.addData('min_correct', min_correct)&amp;#10;thisExp.addData('earnings', earnings)&amp;#10;&amp;#10;# Update and Reset&amp;#10;counter = counter + 1&amp;#10;&amp;#10;&amp;#10;" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each Frame" updates="constant" val="n = len(theseKeys)&amp;#10;i = 0&amp;#10;while i &lt; n:&amp;#10;    if theseKeys[i] == 'return' and len(inputText) &gt; 3:&amp;#10;        # pressing RETURN means time to stop&amp;#10;        continueRoutine = False&amp;#10;        break&amp;#10;&amp;#10;    elif theseKeys[i] == 'backspace':&amp;#10;        inputText = inputText[:-1] #lose the final character&amp;#10;        i = i + 1&amp;#10;    &amp;#10;    elif theseKeys[i] == 'period':&amp;#10;        inputText  = inputText + &quot;.&quot;&amp;#10;        i = i+1&amp;#10;&amp;#10;    elif theseKeys[i] == 'minus':&amp;#10;        inputText  = inputText + &quot;-&quot;&amp;#10;        i = i+1&amp;#10;&amp;#10;    elif theseKeys[i] in ['lshift', 'rshift']:&amp;#10;        shift_flag = True&amp;#10;        i = i+1&amp;#10;&amp;#10;    else:&amp;#10;        if len(theseKeys[i]) ==1:&amp;#10;            #we only have 1 char so should be a normal key&amp;#10;            #otherwise it might be 'ctrl' or similar so ignore it&amp;#10;            if shift_flag:&amp;#10;                inputText += chr( ord(theseKeys[i]) - ord(' '))&amp;#10;                shift_flag = False&amp;#10;            else:&amp;#10;                inputText += theseKeys[i]&amp;#10;        i = i + 1" valType="extendedCode"/>
      </CodeComponent>
      <KeyboardComponent name="exitKeys_9">
        <Param name="correctAns" updates="constant" val="" valType="str"/>
        <Param name="storeCorrect" updates="constant" val="False" valType="bool"/>
        <Param name="name" updates="None" val="exitKeys_9" valType="code"/>
        <Param name="stopVal" updates="constant" val="" valType="code"/>
        <Param name="durationEstim" updates="None" val="" valType="code"/>
