    <Routine name="binary_choice_house">
      <CodeComponent name="code_17">
        <Param name="Begin Experiment" updates="constant" val="&amp;#10;" valType="extendedCode"/>
        <Param name="name" updates="None" val="code_17" valType="code"/>
        <Param name="Begin Routine" updates="constant" val="theseKeys=&quot;&quot;&amp;#10;shift_flag = False&amp;#10;&amp;#10;# To skip this segment:&amp;#10;#binary_house.finished = 1&amp;#10;&amp;#10;# Reset index if counter = 9 (9 house exemplars)&amp;#10;if counter == 9:&amp;#10;    counter = 0&amp;#10;    roundNum = roundNum + 1&amp;#10;    correct_num = 0&amp;#10;    earnings = 0&amp;#10;&amp;#10;# For new set of learning trials, create random array of houses&amp;#10;if counter == 0:&amp;#10;    selector = np.arange(9)&amp;#10;    np.random.shuffle(selector)&amp;#10;    correct_num = 0&amp;#10;&amp;#10;binary_house_left = house_df.exemplar[selector[counter]]&amp;#10;value_left = round(house_df.value[selector[counter]], 2)&amp;#10;#the right face is always 5 ahead (loops around)&amp;#10;binary_house_right = house_df.exemplar[selector[((counter + 5)%9)]]&amp;#10;value_right = round(house_df.value[selector[((counter+5)%9)]], 2)&amp;#10;&amp;#10;# If 5 rounds, then STOP the experiment&amp;#10;if roundNum == 5:&amp;#10;    stopText = &quot;STOP \nGET RA&quot;&amp;#10;    learn_image = inputText = &quot;images/exemplars/end_trial.jpg&quot;" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each Frame" updates="constant" val="n = len(theseKeys)&amp;#10;i = 0&amp;#10;&amp;#10;####################&amp;#10;# KEY STROKE INPUT #&amp;#10;####################&amp;#10;while i &lt; n:&amp;#10;&amp;#10;    #for ACCEPTING and starting a new loop&amp;#10;    if theseKeys[i] == 'f':&amp;#10;        selected_house_val = value_left&amp;#10;        unselected_house_val = value_right&amp;#10;        continueRoutine = False&amp;#10;        break&amp;#10;    &amp;#10;    #for REJECTING and starting a new loop&amp;#10;    elif theseKeys[i] == 'j':&amp;#10;        selected_house_val = value_right&amp;#10;        unselected_house_val = value_left&amp;#10;        continueRoutine = False&amp;#10;        break&amp;#10;&amp;#10;    elif theseKeys[i] in ['lshift', 'rshift']:&amp;#10;        shift_flag = True&amp;#10;        i = i+1&amp;#10;&amp;#10;    else:&amp;#10;        if len(theseKeys[i]) ==1:&amp;#10;            #we only have 1 char so should be a normal key&amp;#10;            #otherwise it might be 'ctrl' or similar so ignore it&amp;#10;            if shift_flag:&amp;#10;                inputText += chr( ord(theseKeys[i]) - ord(' '))&amp;#10;                shift_flag = False&amp;#10;            else:&amp;#10;                inputText += theseKeys[i]&amp;#10;        i = i + 1" valType="extendedCode"/>
