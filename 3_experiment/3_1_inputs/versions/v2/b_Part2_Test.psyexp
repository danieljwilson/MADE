        <Param name="name" updates="None" val="code_10" valType="code"/>
        <Param name="Begin Routine" updates="constant" val="theseKeys=&quot;&quot;&amp;#10;shift_flag = False&amp;#10;input_text_faceVal.alignHoriz = 'center'&amp;#10;&amp;#10;min_correct = 16&amp;#10;&amp;#10;# Reset index if counter = 18 (9 face/house exemplars each)&amp;#10;if counter == 18:&amp;#10;    counter = 0&amp;#10;    roundNum = roundNum + 1&amp;#10;    correct_num = 0&amp;#10;    face_count = 0&amp;#10;    house_count = 0&amp;#10;    total_earnings = 0&amp;#10;&amp;#10;# For new set of learning trials, create random array of faces&amp;#10;if counter == 0:&amp;#10;    selector1 = np.arange(9)&amp;#10;    np.random.shuffle(selector1)&amp;#10;    selector2 = np.arange(9)&amp;#10;    np.random.shuffle(selector2)&amp;#10;    correct_num = 0&amp;#10;&amp;#10;if counter%2 == 0:&amp;#10;    learn_image = house_df.exemplar[selector1[house_count]]&amp;#10;    actualValue = round(house_df.value[selector1[house_count]], 2)&amp;#10;    house_count = house_count +1&amp;#10;else:&amp;#10;    learn_image = face_df.exemplar[selector2[face_count]]&amp;#10;    actualValue = round(house_df.value[selector2[face_count]], 2)&amp;#10;    face_count = face_count +1&amp;#10;&amp;#10;# If more than 4 rounds, then STOP the experiment (if roundNum = 5)&amp;#10;if roundNum == 5:&amp;#10;    stopText = &quot;STOP \nGET RA&quot;&amp;#10;    learn_image = inputText = &quot;images/exemplars/end_trial.jpg&quot;&amp;#10;" valType="extendedCode"/>
